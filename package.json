{
  "name": "premedium",
  "version": "1.0.0",
  "description": "Premedium is platform for publication and reading articles",
  "main": "index.js",
  "scripts": {
    "sim": "ts-node src/index.ts",
    "dev:server": "nodemon",
    "dev": "concurrently \"npm run dev:server\" \"cd client && npm start\"",
    "start": "node dist/index.js",
    "build": "tsc && cd client && npm run build",
    "db:migrate": "sequelize db:migrate",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier --config package.json --write ./**/*.jsx ./**/*.js",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:js": "eslint ./**/*.ts --fix --format stylish --color",
    "lint:css": "stylelint ./**/*.scss --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/premedium/premedium.git"
  },
  "author": "Premedium team: HollyBang and Rammfall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/premedium/premedium/issues"
  },
  "homepage": "https://github.com/premedium/premedium#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.6.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/validator": "^10.11.3",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "browserslist": "^4.6.6",
    "concurrently": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-typescript": "^5.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.42",
    "typescript": "^3.6.3"
  },
  "eslintIgnore": [
    "public",
    "dist",
    "**/migrations/",
    "client"
  ],
  "lint-staged": {
    "*.{js, jsx}": "eslint",
    "*.scss": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged jest",
      "pre-push": "lint-staged"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread",
      "@babel/plugin-transform-typescript"
    ]
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "express": "^4.17.1",
    "googleapis": "^43.0.0",
    "jest": "^24.8.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.12.0",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^2.6.3",
    "sequelize": "^5.19.0",
    "sequelize-cli": "^5.5.0",
    "uuid": "^3.3.3",
    "validator": "^11.1.0"
  }
}
